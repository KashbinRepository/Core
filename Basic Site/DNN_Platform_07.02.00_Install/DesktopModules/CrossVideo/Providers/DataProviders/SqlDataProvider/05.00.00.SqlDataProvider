IF NOT EXISTS (SELECT * FROM sys.objects WHERE object_id = OBJECT_ID(N'{databaseOwner}[{objectQualifier}CrossVideo_UDData_GetByFieldAndVideo]') AND type in (N'P', N'PC'))
