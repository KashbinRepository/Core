	[RecommendCount],
	[RecommendUp],
	[RecommendDown],
	[DownloadRoles],
	[Private],
	[Protected],
	[ProtectPassword],
    [HasVideo]
) VALUES (
	@TypeId,
	@PortalId,
	@UserId,
	@UserName,
	GetDate(),
	@Quote,
	@Title,
	@Author,
	@Source,
	@Thumbnail,
	@Image,
	@LinkUrl,
	@Summary,
	@Description,
	@Field1,
	@Field2,
	@Field3,
	@Field4,
