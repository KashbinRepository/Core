EXEC dbo.sp_executesql @statement = N'

Create PROCEDURE {databaseOwner}[{objectQualifier}CrossVideo_UDData_GetByFieldAndVideo]
	@FieldId int,
    @VideoId int
	
AS
set rowcount 1
SELECT *
FROM {databaseOwner}{objectQualifier}CrossVideo_UDData
WHERE
	[FieldId] = @FieldId and
    [VideoId]=@VideoId
	




' 
END
